{% extends 'SiteBundle::layout.html.twig' %}

{% block body %}
<ul class="content">
	{% for item in rs %}
		<li class="photo">
			<a href="#" class="box">
				<div class="time">
					<div class="clock"></div>
					<div class="tmp">10m atr√°s</div>
				</div>
				<div style="position:absolute; z-index:1;">
					<img src="{{ item.coverPic }}" />
				</div>
				<div class="info">
					<h2 class="title">{{ item.name }}</h2>
					<div class="more">25 Spooters - 2 Friends</div>
				</div>
			</a>
			<div class="back" onclick="location.href='#';"></div>
		</li>
	{% endfor %}
</ul>
<div class="create" style="height:1px; display:block;"></div>
{% endblock %}
