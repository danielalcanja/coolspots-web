<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CoolSpots\SiteBundle\Entity\CsGeo" table="cs_geo">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="countryCode" type="string" column="country_code" length="10" nullable="false"/>
    <field name="city" type="string" column="city" length="50" nullable="false"/>
    <field name="region" type="integer" column="region" nullable="false"/>
    <field name="regionName" type="string" column="region_name" length="50" nullable="false"/>
    <field name="latitude" type="string" column="latitude" length="20" nullable="false"/>
    <field name="longitude" type="string" column="longitude" length="20" nullable="false"/>
  </entity>
</doctrine-mapping>
