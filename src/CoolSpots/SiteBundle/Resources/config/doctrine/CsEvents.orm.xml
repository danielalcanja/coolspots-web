<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CoolSpots\SiteBundle\Entity\CsEvents" table="cs_events">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateAdded" type="datetime" column="date_added" nullable="true"/>
    <field name="name" type="string" column="name" length="100" nullable="true"/>
    <field name="tag" type="string" column="tag" length="100" nullable="true"/>
    <field name="coverPic" type="string" column="cover_pic" length="150" nullable="true"/>
    <field name="lastMinId" type="string" column="last_min_id" length="100" nullable="true"/>
    <field name="nextMaxId" type="string" column="next_max_id" length="100" nullable="true"/>
    <many-to-one field="idLocation" target-entity="CsLocation">
      <join-columns>
        <join-column name="id_location" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idGeo" target-entity="CsGeo">
      <join-columns>
        <join-column name="id_geo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
